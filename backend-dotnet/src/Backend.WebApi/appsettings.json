{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Database": {
    "AutoMigrations": true,
    "ConnectionString": "Data Source=app-prod.db"
  },
  "Session": {
    "Lifetime": 180,
    "Domain": "localhost"
  }, 
  "Cors": {
    "Origins": ["http://localhost:3000", "https://google.com"],
    "Methods": ["GET", "POST", "DELETE", "OPTIONS"],
    "Headers": ["Content-Type"],
    "AllowCredentials": true
  }
}
